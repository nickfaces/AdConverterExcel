<IconListItem>

    IconLeftWidget:
        icon: root.icon

<SettingsScreen>

    MDBoxLayout:
        orientation: 'vertical'
        spacing: 10
        padding: 10
        MDBoxLayout:
            orientation: 'horizontal'
            spacing: 10
            padding: 10
            MDLabel:
                text: 'Поле импорта'
                theme_text_color: "Custom"
                text_color: app.theme_cls.primary_color
